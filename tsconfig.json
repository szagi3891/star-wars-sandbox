{
	"compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
		"moduleResolution": "node",
		"strict": true,
        "lib": ["dom", "es6", "esnext"],
        "jsx": "react",
		"downlevelIteration": true,
		"sourceMap": true,
		"declaration": true,
		"experimentalDecorators": true,
		"noUnusedLocals": true,
        "noUnusedParameters": true,
		"outDir": "./dist"
	}
}
